@model LecturesClaimingSystem.Models.Claim

<div>
    <h4>Your claim has been submitted successfully!</h4>
    <p>Claim ID: @Model.ClaimId</p>
    <p>Lecturer Name: @Model.LecturerName</p>
    <p>Hours Worked: @Model.HoursWorked</p>
    <p>Hourly Rate: @Model.HourlyRate</p>
    <p>Description: @Model.Description</p>
    <p>Status: @Model.Status</p>
    <p>Submission Date: @Model.SubmissionDate.ToString("g")</p>
</div>

<a href="@Url.Action("Dashboard", "ProgrammeCoordinator")">Back to Dashboard</a>
